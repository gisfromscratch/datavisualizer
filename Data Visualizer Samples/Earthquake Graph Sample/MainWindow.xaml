<Window x:Class="Earthquake_Graph_Sample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title="Earthquake Graph Sample" Height="600" Width="800">
    <Grid>
        <d3:ChartPlotter Name="plotter">
            <d3:ChartPlotter.HorizontalAxis>
                <d3:HorizontalDateTimeAxis Name="dateAxis"/>
            </d3:ChartPlotter.HorizontalAxis>

            <d3:Header FontFamily="Helvetica" Content="Earthquakes"/>
            <d3:VerticalAxisTitle FontFamily="Helvetica" Content="Magnitude"/>
            <d3:HorizontalAxisTitle FontFamily="Helvetica" Content="Date"/>
        </d3:ChartPlotter>
    </Grid>
</Window>
